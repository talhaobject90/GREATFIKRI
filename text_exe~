ECHO 'REPLACING SETTINGS FILE....'


echo UPDATING DATABASE..................
mysql -h $OPENSHIFT_DB_HOST -P $OPENSHIFT_DB_PORT -u $OPENSHIFT_DB_USERNAME --password="$OPENSHIFT_DB_PASSWORD" great < /var/lib/openshift/543953795973ca3e770001b7/app-root/runtime/repo/great.sql
 
echo DATABSE UPDATED !

